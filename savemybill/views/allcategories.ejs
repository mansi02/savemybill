<%- include ./Partials/header %>
<%- include ./Partials/navigationalbar %>
<div class="container">
    <div class="row">
        <h1>Category list</h1>
        <table class="table table-striped table-primary">
            <thead>
                <tr>
                    <th>Category Name</th>
                    <th>Delete</th>
                </tr>
            </thead>
            <tbody>
                <% if(record.length >0) { 
            record.forEach(function(row){
            %>
                <tr>
                    <td><%= row.CategoryName %></td>
                     <td><!--<a class="btn btn-primary-xs" href="/allcategories/edit/<%= row._id %>"><i
                                class="fa fa-edit"></i></a> -->
                        <a class="btn btn-danger-xs" href="/allcategories/delete/<%= row._id %>"><i
                                class="fa fa-minus-circle"></i></a></td>
                </tr>
                <% }) } %>
            </tbody>
        </table>
    </div>
</div>
<%- include ./Partials/footer %>